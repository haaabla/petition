<h2>Edit Profile loco :O</h2>

<form id="data" name="data" method="post">
    Firstname:<br>
    <input id="firstname"type="text" name="firstname" value="{{firstname}}">
    <br>Lastname:<br>
    <input id="lastname" type="text" name="lastname" value="{{lastname}}">
    <br>Email:<br>
    <input id="email" type="email" name="email" value="{{email}}">
    <br>Password:<br>
    <input id="password" type="password" name="password" value="">
    <br>Age:<br>
    <input id="age" type="text" name="age" value="{{age}}">
    <br>City:<br>
    <input id="city" type="text" name="city" value="{{city}}">
    <br>Homepage:<br>
    <input id="url" type="text" name="url" value="{{url}}">
    <br>
    <input id="update" type="submit" value="Submit">
</form>
