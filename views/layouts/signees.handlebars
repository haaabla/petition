<main class="view-all">

    <h2>Likeminded Beings</h2>
    <img class="x" src="/images/x.png" alt="">

    <ul class="signee-list">
        {{#each list}}
            <li>
                {{#if url}}
                    <a href="http://{{url}}" target="_blank">{{first_name}} {{last_name}}</a>
                {{else}}
                    <h6>{{first_name}} {{last_name}}</h6>
                {{/if}}
                {{#if city}}
                    <h5>{{city}}</h5>
                {{else}}
                    {{#if age}}){{/if}}
                {{/if}}
            </li>
        {{/each}}
    </ul>
</main>
